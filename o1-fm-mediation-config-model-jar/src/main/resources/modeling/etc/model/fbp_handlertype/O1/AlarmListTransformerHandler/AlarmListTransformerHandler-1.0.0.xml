<handler:HandlerTypeDefinition oss_common:ns="O1"
    oss_common:name="AlarmListTransformerHandler" oss_common:version="1.0.0"
    xmlns:flow="urn:com:ericsson:schema:xml:oss:fbp_flow"
    xmlns:handler="urn:com:ericsson:schema:xml:oss:fbp_handlertype"
    xmlns:oss_common="urn:com:ericsson:schema:xml:oss:oss_common"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <oss_common:modelCreationInfo author="TeamAmerica/Doozers"
        creationDate="03/11/2023">
        <oss_common:designedModel componentId="AlarmListTransformerHandler"/>
    </oss_common:modelCreationInfo>

    <oss_common:desc>Handler responsible for transforming O1 alarm lists.</oss_common:desc>

    <flow:handler>
        <flow:className>com.ericsson.oss.mediation.fm.o1.handlers.AlarmListTransformerHandler</flow:className>
        <flow:attributeDef oss_common:name="fdn">
            <oss_common:desc>FDN of the FmAlarmSupervision MO
            </oss_common:desc>
            <oss_common:type xsi:type="oss_common:stringType"/>
            <flow:source uri="dps:fdn"/>
        </flow:attributeDef>
    </flow:handler>

</handler:HandlerTypeDefinition>


